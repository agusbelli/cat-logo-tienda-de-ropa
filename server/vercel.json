{
    "version": 2,
    "name": "catalogo-carcassurban-server",
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "build": {
      "commands": {
        "prisma:migrate": "npm run prisma:migrate",
        "prisma:generate": "npm run prisma:generate"
      }
  }
}